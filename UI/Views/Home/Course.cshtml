@model List<UI.Models.CourseModel>
@{
    ViewBag.Title = "COURSES";
}
<link href="~/Content/datatables.min.css" rel="stylesheet" />
<table class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>DepID</th>
            <th>Code</th>
            <th>CourseCode</th>
            <th>Name</th>
            <th>ECTS</th>
            <th>Credit</th>
            <th>Level</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.depId</td>
                <td>@item.code</td>
                <td>@item.codename</td>
                <td>@item.name</td>
                <td>@item.ects</td>
                <td>@item.credit</td>
                <td>@item.level</td>
                <td>@item.type</td>
            </tr>
        }
    </tbody>
</table>
<script src="~/Scripts/datatables.min.js"></script>
<script>
    $("table").DataTable();
</script>