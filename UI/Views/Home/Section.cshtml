@model List<UI.Models.SectionModel>
@{
    ViewBag.Title = "SECTION";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/datatables.min.css" rel="stylesheet" />
<table class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>DepID</th>
            <th>CourseID</th>
            <th>SectionID</th>
            <th>Instructor</th>
            <th>DateInfo</th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.department</td>
                <td>@item.course</td>
                <td>@item.sectionId</td>
                <td>@item.instructor</td>
                @foreach (var dateitem in item.dateinfo)
                {
                    <td>@dateitem.day @dateitem.start @dateitem.end @dateitem.place</td>
                }
                @for (int i = 0; i < 5-item.dateinfo.Count; i++)
                {
                    <td></td>
                }
            </tr>
        }
    </tbody>
</table>
<script src="~/Scripts/datatables.min.js"></script>
<script>
    $("table").DataTable();
</script>